<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vcimpquote">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.VCIMPQuote">
        <activity android:name=".quote.QuoteDetailActivity"></activity>
        <activity android:name=".quote.QuoteListActivity" />
        <activity android:name=".quote.QuoteActivity" />
        <activity android:name=".vendor.VendorActivity" />
        <activity android:name=".vendor.VendorDetailActivity" />
        <activity android:name=".vendor.VendorListActivity" />
        <activity android:name=".customer.CustomerActivity" />
        <activity android:name=".customer.CustomerDetailActivity" />
        <activity android:name=".customer.CustomerListActivity" />
        <activity android:name=".car.CarActivity" />
        <activity android:name=".car.CarDetailActivity" />
        <activity android:name=".car.CarListActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>